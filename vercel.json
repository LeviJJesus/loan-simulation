{
  "version": 2,
  "builds": [
    {
      "src": "loan-simulation/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/send-email",
      "dest": "/loan-simulation/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/loan-simulation/public/$1"
    }
  ]
}
